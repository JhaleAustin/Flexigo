<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#020202"
    tools:context=".PhysicalFitness">

    <Button
        android:id="@+id/button13"
        android:layout_width="210dp"
        android:layout_height="60dp"
        android:layout_marginTop="4dp"
        android:text="INPUT RESULTS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12" />

    <Button
        android:id="@+id/button11"
        android:layout_width="210dp"
        android:layout_height="60dp"
        android:layout_marginTop="4dp"
        android:text="MUSCULAR ENDURANCE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button10" />

    <Button
        android:id="@+id/button12"
        android:layout_width="210dp"
        android:layout_height="60dp"
        android:layout_marginTop="4dp"
        android:text="FLEXIBILITY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button11" />

    <Button
        android:id="@+id/button10"
        android:layout_width="210dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:text="AEROBIC FITNESS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="309dp"
        android:layout_height="93dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="B A S E D O N
"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/button13"
        tools:layout_editor_absoluteX="57dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="297dp"
        android:layout_height="143dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="PHYSICAL FIT
ASSESMENT TEST
"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="539dp"
        android:layout_height="217dp"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:srcCompat="@drawable/image__6_"
        tools:layout_editor_absoluteX="-62dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>